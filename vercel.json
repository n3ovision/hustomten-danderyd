{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\.(png|jpg|jpeg|gif|svg|ico|css|js))",
      "dest": "/$1"
    },
    {
      "src": "/(.*\.html)",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
